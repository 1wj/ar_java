<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.cn.bj.dao.CardDao">
    <select id="selectByCardId" resultType="org.cn.bj.domain.Card">
        select * from my_card where cardid=#{cardid}
    </select>

</mapper>