<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.iweb.dao.DeptDao">
    <select id="findDeptById" resultType="com.iweb.model.Dept">
        select * from dept_inf where id=#{id}
    </select>

    <select id="findDepts" resultType="com.iweb.model.Dept">
        select * from dept_inf
    </select>
</mapper>